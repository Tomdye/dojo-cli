{
	"version": "1.8.10",
	"compilerOptions": {
		"declaration": false,
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"target": "es6"
	},
	"filesGlob": [
		"./typings/index.d.ts",
		"./src/commands/*.ts",
		"./src/util/*.ts",
		"./src/lib/*.ts",
		"./tests/**/*.ts"
	],
	"files": [
		"./typings/index.d.ts",
		"./src/commands/install.ts",
		"./src/commands/newApp.ts",
		"./src/util/gitModule.ts",
		"./src/util/path.ts",
		"./src/util/template.ts",
		"./src/lib/cli.ts"
	]
}
